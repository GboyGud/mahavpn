unzip /root/LED_B860H_v5.zip -d /usr/bin
chmod +x /usr/bin/bv5led
chmod +x /usr/bin/bv5ledon
rm /root/LED_B860H_v5.zip
sed -i '$ i\sleep 20 && /usr/bin/bv5led -r' /etc/rc.local
/usr/bin/bv5led -r
rm /root/led
clear
echo -e "499 led green"
echo -e "447 led spdif"
echo -e "448 usb"
